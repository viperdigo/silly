{% extends '::content.html.twig' %}

{% block contentTitle %}{{ "Parameter"|trans }}{% endblock %}
    {% block contentBody %}

        <div class="portlet box blue-hoki">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-plus"></i>{{ "New"|trans }}
                </div>
            </div>
            <div class="portlet-body form">
                {{ form_start(form, {'attr':{'class':'customForm form-horizontal'}}) }}
                <div class="form-body">
                    <div class="row">
                        <div class="col-md-12">
                            {% for child in form %}
                                {{ form_row(child) }}
                            {% endfor %}
                        </div>

                    </div>
                </div>
                <div class="form-actions">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="row">
                                <div class="col-md-1" style="float: right; margin-right: 15px;">
                                    <button type="submit" class="btn blue-hoki">{{ "Save" | trans }}</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                {{ form_end(form) }}
            </div>
        </div>

    {% endblock %}