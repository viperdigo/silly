        'bundles/backend/metronic/assets/admin/layout4/css/layout.css'
        'bundles/backend/metronic/assets/admin/layout4/css/themes/default.css'
        'bundles/backend/metronic/assets/admin/layout4/css/custom.css' %}

        <link rel="stylesheet" type="text/css" href="{{ asset_url }}"/>
        {% endstylesheets %}

        {% block pageCss %}{% endblock %}

        <style>{% block pageInlineCss %}{% endblock %}</style>


        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}"/>

    {% endblock %}

</head>
<body class="{% block bodyClass %}{% endblock %}">

{% block body %}
    {% block copyright %}
        <div class="page-footer">
            <div class="page-footer-inner">
                2015 &copy; Silly by Rodrigo Alfieri.
            </div>
            <div class="scroll-to-top">
                <i class="icon-arrow-up"></i>
            </div>
        </div>
    {% endblock %}
{% endblock %}

{#<body class="page-header-fixed page-sidebar-closed-hide-logo ppage-sidebar-closed-hide-logo">#}

